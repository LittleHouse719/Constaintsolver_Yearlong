{
  "objects": {
    "default": "node",
    "predefine": {
      "key": [
        "key0",
        "key1",
        "key2",
        "key3",
        "key4",
        "key5",
        "key6",
        "key7",
        "key8"
      ],
      "triangle": [
        "triangle"
      ],
      "diamond": [
        "diamond"
      ],
      "square": [
        "square"
      ],
      "circle": [
        "circle"
      ]
    },
    "customobj": [
      "robot"
    ]
  },
  "shape": {
    "node": {
      "prefab": "circle",
      "showName": true,
      "x": false,
      "y": false,
      "color": false,
      "width": 80,
      "height": 80
    },
    "robot": {
      "prefab": "circle",
      "showName": true,
      "x": false,
      "y": false,
      "color": {
        "r": 255,
        "g": 255,
        "b": 0,
        "a": 1
      },
      "width": 40,
      "height": 40
    },
    "triangle": {
      "prefab": "triangle",
      "showName": false
    },
    "diamond": {
      "prefab": "diamond",
      "showName": false
    },
    "square": {
      "prefab": "square",
      "showName": false
    },
    "circle": {
      "prefab": "circle",
      "showName": false
    },
    "key": {
      "prefab": "key",
      "showName": false
    }
  },
  "predicates_rules": {
    "locked": {
      "rules": [
        "rule1"
      ],
      "rule1": {
        "0": "color",
        "value": {
          "equal": "{'r': 255,'g': 0,'b': 0, 'a': 1}"
        }
      }
    },
    "open": {
      "rules": [
        "rule1"
      ],
      "rule1": {
        "0": "color",
        "value": {
          "equal": "{'r': 0,'g': 255,'b': 0, 'a': 1}"
        }
      }
    },
    "arm-empty ": {
      "left": "robot",
      "rules": [
        "rule1"
      ],
      "rule1": {
        "0": "color",
        "value": {
          "equal": "{'r': 0,'g': 255,'b': 0, 'a': 1}"
        }
      }
    },
    "key-shape ": {
      "rules": [
        "rule1"
      ],
      "rule1": {
        "0": "prefab",
        "1": "prefab",
        "value": {
          "equal": "1"
        }
      }
    },
    "lock-shape": {
      "rules": [
        "rule1"
      ],
      "rule1": {
        "0": "prefab",
        "1": "prefab",
        "value": {
          "equal": "1"
        }
      }
    },
    "at": {
      "rules": [
        "rule1",
        "rule2"
      ],
      "rule1": {
        "0": "x",
        "1": "x",
        "value": {
          "equal": "1"
        }
      },
      "rule2": {
        "0": "y",
        "1": "y",
        "value": {
          "equal": "1"
        }
      }
    },
    "at-robot": {
      "left": "robot",
      "rules": [
        "rule1",
        "rule2"
      ],
      "rule1": {
        "0": "x",
        "1": "x",
        "value": {
          "equal": "1"
        }
      },
      "rule2": {
        "0": "y",
        "1": "y",
        "value": {
          "equal": "1"
        }
      }
    },
    "holding": {
      "right": "robot",
      "rules": [
        "rule1",
        "rule2"
      ],
      "rule1": {
        "0": "x",
        "1": "x",
        "value": {
          "equal": "1"
        }
      },
      "rule2": {
        "0": "y",
        "1": "y",
        "value": {
          "equal": "1"
        }
      }
    },
    "place": {
      "rules": [
        "rule1",
        "rule2"
      ],
      "rule1": {
        "0": "x",
        "value": {
          "function": "distribute_grid_around_pointx"
        }
      },
      "rule2": {
        "0": "y",
        "value": {
          "function": "distribute_grid_around_pointy"
        }
      }
    },
     "conn": {
      "rules": [
        "rule1"
      ],
      "rule1": {
        "require":{
          "0":["x","y"],
          "1":["x","y"]
        },
        "action": {
          "function": "draw_line"
        }
      }
    }
  }
}